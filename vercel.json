{
  "cleanUrls": true,
  "redirects": [
    { "source": "/", "destination": "/freezer-inventory/" }
  ],
  "headers": [
    {
      "source": "/freezer-inventory/manifest.webmanifest",
      "headers": [
        { "key": "Content-Type", "value": "application/manifest+json" }
      ]
    },
    {
      "source": "/freezer-inventory/service-worker.js",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache" }
      ]
    }
  ]
}
